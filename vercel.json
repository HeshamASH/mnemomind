{
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python",
      "excludeFiles": "node_modules/**,dist/**,__pycache__/**,requirements.txt"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
