{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "functions": {
    "api/main.py": {
      "runtime": "@vercel/python@5.0.10",
      "maxDuration": 30,
      "excludeFiles": [
        "node_modules/**",
        "dist/**",
        "__pycache__/**",
        "*.pyc",
        "requirements.txt",
        ".env*",
        ".git/**",
        "fixed_documents/**",
        "*.md",
        ".cache/**",
        "/tmp/**",
        "/vercel/.cache/**",
        ".venv/**",
        "**/site-packages/torch/**",
        "**/site-packages/transformers/**",
        "**/sentence_transformers/models/**"
      ]
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "outputDirectory": "dist"
}

